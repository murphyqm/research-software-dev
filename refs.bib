}

@ARTICLE{McLaren2019-pw,
  title    = "{Gravitational instabilities in binary granular materials}",
  author   = "McLaren, Christopher P and Kovar, Thomas M and Penn, Alexander and
              Müller, Christoph R and Boyce, Christopher M",
  journal  = "Proceedings of the National Academy of Sciences",
  volume   =  116,
  number   =  19,
  pages    = "9263--9268",
  abstract = "The motion and mixing of granular media are observed in several
              contexts in nature, often displaying striking similarities to
              liquids. Granular dynamics occur in geological phenomena and also
              enable technologies ranging from pharmaceuticals production to
              carbon capture. Here, we report the discovery of a family of
